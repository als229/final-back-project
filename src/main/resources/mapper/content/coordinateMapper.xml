<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kh.finalproject.content.model.mapper.CoordinateMapper">

  <insert id="insertCoordinate"
  		  parameterType="com.kh.finalproject.content.model.vo.Coordinate">
    INSERT INTO TB_MAP (
      CONTENT_ID,
      MAP_X,
      MAP_Y
    ) VALUES (
      #{contentId},
      #{mapX},
      #{mapY}
    )
  </insert>

</mapper>