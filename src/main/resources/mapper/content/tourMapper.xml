<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kh.finalproject.content.model.dao.TourMapper">

  <insert id="insertTour" parameterType="com.kh.finalproject.content.model.vo.Tour">
    INSERT INTO TB_TOUR (
      CONTENT_ID,
      TOUR_EXP,
      USETIME_TOUR,
      PARKING
    ) VALUES (
      #{contentId},
      #{tourExp},
      #{usetimeTour},
      #{parking}
    )
  </insert>

</mapper>